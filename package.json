{
  "name": "move-away",
  "version": "1.0.1",
  "main": "src/electron-starter.js",
  "private": false,
  "homepage": "./",
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "react-start": "react-scripts start | cat",
    "start": "concurrently 'npm run react-start' 'wait-on http://localhost:3000/ && npm run electron-dev .'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.3",
    "node-sass": "^4.11.0",
    "wait-on": "^3.2.0"
  }
}
